/* Keyframes cho hiệu ứng lắc lư */
@keyframes sway {
    0%, 100% {
        transform: rotate(-3deg);
    }
    50% {
        transform: rotate(3deg);
    }
}

/* Áp dụng hiệu ứng cho banner */
@media screen and (min-width: 1024px) {
    .banner-sidebar img {
        animation: sway 2s ease-in-out infinite;
        transform-origin: top center; /* Đảm bảo rằng phần trên cùng của banner không di chuyển */
    }
}


/* Định dạng cho banner chỉ trên desktop */
@media screen and (min-width: 1024px) {
    .banner-sidebar {
        position: fixed;
        top: 50%;
        transform: translateY(-50%);
        z-index: 100;
    }

    .banner-sidebar.left {
        left: 5%;
    }

    .banner-sidebar.right {
        right: 5%;
    }

    .banner-sidebar img {
        width: 132px;
        height: 367.2px;
    }

